		<constructor-arg ref="metadataOData4j" />
	</bean>
	
	<bean id="T24ReverseEntity" class="com.temenos.interaction.commands.t24.T24ReverseCommand">
		<constructor-arg ref="metadata" />
		<constructor-arg ref="t24Config" />
	</bean>
	
	<bean id="T24RestoreEntity" class="com.temenos.interaction.commands.t24.T24RestoreCommand">
		<constructor-arg ref="metadata" />
		<constructor-arg ref="t24Config" />
	</bean>
	
	<bean id="T24ReviewEntity" class="com.temenos.interaction.commands.t24.T24AuditorReviewCommand">
		<constructor-arg ref="metadata" />
		<constructor-arg ref="t24Config" />
	</bean>
	
	<bean id="T24VerifyEntity" class="com.temenos.interaction.commands.t24.T24VerifyCommand">
		<constructor-arg ref="metadata" />
		<constructor-arg ref="t24Config" />
	</bean>

	<bean id="T24EntityMetadata" class="com.temenos.interaction.commands.t24.T24EntityMetadataCommand">
		<constructor-arg ref="metadata" />
		<constructor-arg ref="t24Config" />
	</bean>
	
    <bean id="T24Validation" class="com.temenos.interaction.commands.t24.T24ValidationCommand">
        <constructor-arg ref="metadata" />
        <constructor-arg ref="t24Config" />
        <constructor-arg ref="metadataOData4j" />
    </bean>

   	<bean id="T24FieldMetadata" class="com.temenos.interaction.commands.t24.T24FieldMetadataCommand">
        <constructor-arg ref="metadata" />
        <constructor-arg ref="metadataOData4j" />
        <constructor-arg ref="t24Config" />
    </bean>
    
    <bean id="GETNoop" class="com.temenos.interaction.core.command.NoopGETCommand" />

	<bean id="T24CreateEntityCommand" class="com.temenos.interaction.commands.t24.T24GetNewEntityCommand" >
		<constructor-arg ref="metadata" />
		<constructor-arg ref="t24Config" />
	</bean>
	
	<bean id="GETEntities" class="${retryWorkflowClass}">
		<constructor-arg ref="IRISGETEntities" />
		<constructor-arg value="${maxRetryCount}" />
		<constructor-arg value="${maxRetryInterval}" />		
	</bean>
	
	<bean id="GETEntity" class="${retryWorkflowClass}">
		<constructor-arg ref="IRISGETEntity" />
		<constructor-arg value="${maxRetryCount}" />
		<constructor-arg value="${maxRetryInterval}" />		
	</bean>
	
	<bean id="GETNavProperty" class="${retryWorkflowClass}">
		<constructor-arg ref="IRISGETNavProperty" />
		<constructor-arg value="${maxRetryCount}" />
		<constructor-arg value="${maxRetryInterval}" />		
	</bean>
	
	<bean id="GetLiveEntities" class="${retryWorkflowClass}">
		<constructor-arg ref="T24GetLiveEntities" />
		<constructor-arg value="${maxRetryCount}" />
		<constructor-arg value="${maxRetryInterval}" />
	</bean>
	
	<bean id="GetLiveEntity" class="${retryWorkflowClass}">
		<constructor-arg ref="T24GetLiveEntity" />
		<constructor-arg value="${maxRetryCount}" />
		<constructor-arg value="${maxRetryInterval}" />		
	</bean>

	<bean id="GetIauthEntities" class="${retryWorkflowClass}">
		<constructor-arg ref="T24GetIauthEntities" />
		<constructor-arg value="${maxRetryCount}" />
		<constructor-arg value="${maxRetryInterval}" />		
	</bean>

	<bean id="GetIauthEntity" class="${retryWorkflowClass}">
