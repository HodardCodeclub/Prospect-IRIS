	<bean id="T24InputEntity" class="com.temenos.interaction.commands.t24.T24InputCommand">
		<constructor-arg ref="metadata" />
		<constructor-arg ref="t24Config" />
		<constructor-arg ref="metadataOData4j" />
	</bean>
	
	<bean id="T24CreateEntity" class="com.temenos.interaction.commands.t24.T24GetNewEntityCommand">
