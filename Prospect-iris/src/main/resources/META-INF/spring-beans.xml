	<!-- Image download (EFS) -->
  	<!--
  	<bean id="downloadProviderFS" class="com.efstech.temenos.inputstreamprovidertocf.PDMInputStreamProviderTOCFTestServlet" />
 	-->
    	
	<!-- TEMENOS Interface -->
	<bean id="downloadProviderFS" class="com.temenos.tocf.datatransfer.file.TOCFDocXferFileInputStreamProvider" />
  	<!--
	<bean id="downloadProviderDB" class="com.temenos.tocf.datatransfer.db.TOCFDocXferDbInputStreamProvider" />
	<bean id="uploadProviderFS" class="com.temenos.tocf.datatransfer.file.TOCFDocXferFileOutputStreamWriter" />
	<bean id="uploadProviderDB" class="com.temenos.tocf.datatransfer.db.TOCFDocXferDbOutputStreamWriter" />
 	-->
	<bean id="T24ImageDownloadFS" class="com.temenos.interaction.commands.t24.datatransfer.T24ImageDownloadCommandFS">
		<constructor-arg ref="metadata" />
        <constructor-arg ref="metadataOData4j" />
        <constructor-arg ref="downloadProviderFS" />
        <constructor-arg name="downloadPath" value="c:\temp\download" />
	</bean>
	
	<bean id="ImageDownload" class="${retryWorkflowClass}">
